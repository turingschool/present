<h2><%= link_to @module.name, turing_module_path(@module) %></h2>
<h3><%= @attendance.title %></h3>
<h3>Date: <%= @attendance.pretty_date %></h3>
<h3>Attendance Time: <%= @attendance.pretty_time %> <%= link_to "Update Attendance Time", edit_attendance_path(@attendance) %></h3>
<% if @attendance.zoom_attendance %>
  <%= render partial: "shared/zoom_attendance_table", locals: {facade: AttendanceShowFacade.new(@attendance)} %>  
<% else %>
  <%= render partial: "shared/slack_attendance_table", locals: {attendance: @attendance} %>  
<% end %>

